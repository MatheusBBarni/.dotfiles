{
  "editor.tabSize": 2,
  "explorer.autoReveal": false,
  "explorer.compactFolders": false,
  "emmet.includeLanguages": {
    "javascript": "javascriptreact",
    "rescript": "javascriptreact",
    "gohtml": "javascriptreact"
  },
  "workbench.colorCustomizations": {
    "statusBar.debuggingBackground": "#D08770",
    "statusBar.foreground": "#ECEFF4",
    "statusBar.background": "#4C566A",
    "statusBarItem.hoverBackground": "#f8f8f2",
    "activityBarBadge.background": "#E5E9F0",
    "badge.background": "#D8DEE9",
    "badge.foreground": "#f8f8f2"
  },
  "editor.bracketPairColorization.enabled": true,
  "editor.minimap.enabled": false,
  "editor.parameterHints.enabled": false,
  "editor.maxTokenizationLineLength": 999999,
  "editor.fontLigatures": true,
  "editor.formatOnSave": true,
  "editor.codeActionsOnSave": {
    "source.fixAll.eslint": "explicit"
  },
  "editor.defaultFormatter": "esbenp.prettier-vscode",
  "eslint.validate": [
    "javascript",
  ],
  "files.eol": "\n",
  "workbench.sideBar.location": "right",
  "files.exclude": {
    "**/.git": true,
    "**/.svn": true,
    "**/.hg": true,
    "**/CVS": true,
    "**/.DS_Store": true,
    "**/Thumbs.db": true,
    "**/*.bs.js": true
  },
  "auto-close-tag.activationOnLanguage": [
    "xml",
    "php",
    "blade",
    "ejs",
    "jinja",
    "javascript",
    "javascriptreact",
    "typescript",
    "typescriptreact",
    "plaintext",
    "markdown",
    "vue",
    "liquid",
    "erb",
    "lang-cfml",
    "cfml",
    "HTML (Eex)",
    "rescript"
  ],
  "editor.guides.highlightActiveIndentation": false,
  "[rust]": {
    "editor.semanticHighlighting.enabled": false,
    "editor.defaultFormatter": "rust-lang.rust-analyzer"
  },
  "window.zoomLevel": 2,
  "[javascript]": {
    "editor.defaultFormatter": "vscode.typescript-language-features"
  },
  "[typescriptreact]": {
    "editor.defaultFormatter": "vscode.typescript-language-features"
  },
  "[json]": {
    "editor.defaultFormatter": "vscode.json-language-features"
  },
  "[typescript]": {
    "editor.defaultFormatter": "vscode.typescript-language-features"
  },
  "[jsonc]": {
    "editor.defaultFormatter": "vscode.json-language-features"
  },
  "workbench.colorTheme": "Tokyo Night",
  "editor.fontFamily": "FiraCode Nerd Font Mono",
  "terminal.integrated.defaultProfile.windows": "Git Bash",
  "workbench.iconTheme": "vscode-great-icons",
  "workbench.productIconTheme": "fluent-icons"
}
