{
  "window.zoomLevel": 1,
  "editor.tabSize": 2,
  "explorer.autoReveal": false,
  "explorer.compactFolders": false,
  "emmet.includeLanguages": {
    "javascript": "javascriptreact"
  },
  "workbench.colorCustomizations": {
    "statusBar.debuggingBackground": "#D08770",
    "statusBar.foreground": "#ECEFF4",
    "statusBar.background": "#4C566A",
    "statusBarItem.hoverBackground": "#f8f8f2",
    "activityBarBadge.background": "#E5E9F0",
    "badge.background": "#D8DEE9",
    "badge.foreground": "#f8f8f2",
  },
  "editor.bracketPairColorization.enabled": true,
  "editor.minimap.enabled": false,
  "editor.highlightActiveIndentGuide": false,
  "editor.maxTokenizationLineLength": 999999,
  "editor.fontFamily": "Fira Code",
  "editor.fontLigatures": true,
  "workbench.iconTheme": "material-icon-theme",
  "editor.formatOnSave": true,
  "editor.codeActionsOnSave": {
    "source.fixAll.eslint": true
  },
  "eslint.validate": [
    "javascript"
  ],
  "files.eol": "\n",
  "workbench.colorTheme": "Dracula",
  "workbench.sideBar.location": "right",
  "files.exclude": {
    "**/.git": true,
    "**/.svn": true,
    "**/.hg": true,
    "**/CVS": true,
    "**/.DS_Store": true,
    "**/Thumbs.db": true,
    "**/*.bs.js": true
  },
  "workbench.colorTheme": "Nord",
  "newFile.defaultBaseFileName": "newFile",
  "newFile.relativeTo": "project",
  "newFile.defaultFileExtension": ".ts",
  "newFile.rootDirectory": "~",
  "newFile.showPathRelativeTo": "project",
  "newFile.expandBraces": true,
  "auto-close-tag.activationOnLanguage": [
    "xml",
    "php",
    "blade",
    "ejs",
    "jinja",
    "javascript",
    "javascriptreact",
    "typescript",
    "typescriptreact",
    "plaintext",
    "markdown",
    "vue",
    "liquid",
    "erb",
    "lang-cfml",
    "cfml",
    "HTML (Eex)",
    "rescript"
  ]
}
